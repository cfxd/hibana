{%- assign menu = linklists[section.settings.menu] -%}
<header class="Header" data-module="header">
    {%- include 'header-notification' -%}
    {%- include 'header-banner' -%}
</header>
{% schema %}

{
    "name": "Header",
    "settings": [
        {
            "type": "header",
            "content": "Header"
        }, {
            "type": "link_list",
            "id": "menu",
            "label": "Menu",
            "default": "main-menu"
        }
    ]
}

{% endschema %}
